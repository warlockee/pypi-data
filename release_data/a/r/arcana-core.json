    "0.8.1": {
        "info": {
            "author": "",
            "author_email": "\"Thomas G. Close\" <tom.g.close@gmail.com>",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Environment :: Console",
