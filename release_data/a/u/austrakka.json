    "0.22.0": {
        "info": {
            "author": "AusTrakka Dev Team",
            "author_email": "dev@austrakka.net.au",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Developers",
