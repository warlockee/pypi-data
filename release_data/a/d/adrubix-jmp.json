    "0.1.9": {
        "info": {
            "author": "Advestis",
            "author_email": "pythondev@advestis.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Operating System :: Unix",
                "Programming Language :: Python :: 3.7",
