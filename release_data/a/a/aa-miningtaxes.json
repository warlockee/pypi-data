    "1.1.8": {
        "info": {
            "author": "Arc Tiru",
            "author_email": "arcturusstl@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Environment :: Web Environment",
                "Framework :: Django",
                "Framework :: Django :: 3.2",
