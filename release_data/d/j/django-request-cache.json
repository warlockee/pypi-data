            "author_email": "hnezbeda@anexia-it.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Environment :: Web Environment",
                "Framework :: Django",
                "Framework :: Django :: 3.2",
                "Framework :: Django :: 4.0",
                "Framework :: Django :: 4.1",
