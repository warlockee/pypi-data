        "info": {
            "author": "Simon J.L. Billinge",
            "author_email": "sb2896@columbia.edu",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Environment :: MacOS X",
                "Environment :: Win32 (MS Windows)",
                "Environment :: X11 Applications",
                "Intended Audience :: Science/Research",
