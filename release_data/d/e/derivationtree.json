    "0.3.2": {
        "info": {
            "author": "Dominic Steinhoefel",
            "author_email": "Dominic Steinhoefel <dominic.steinhoefel@cispa.de>",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Science/Research",
