    "0.5.3": {
        "info": {
            "author": "zencore",
            "author_email": "dobetter@zencore.cn",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
