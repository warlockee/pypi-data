    "1.0.2": {
        "info": {
            "author": "Greenroom Robotics",
            "author_email": "team@greenroomrobotics.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Plugins",
                "Intended Audience :: Developers",
                "Programming Language :: Python",
                "Topic :: Software Development :: Build Tools"
