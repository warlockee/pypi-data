    "0.2.5.dev0": {
        "info": {
            "author": "iizegrim",
            "author_email": "iizegrim@protonmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: X11 Applications :: Qt",
