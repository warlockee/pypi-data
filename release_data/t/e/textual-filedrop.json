    "0.2.1": {
        "info": {
            "author": "agmmnn",
            "author_email": "agmmnn@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Programming Language :: Python :: 3",
                "Programming Language :: Python :: 3.10",
