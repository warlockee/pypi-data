    "0.2.1": {
        "info": {
            "author": "Mischa Schindowski",
            "author_email": "mschindowski@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Environment :: Console",
                "Intended Audience :: Developers",
                "License :: OSI Approved :: MIT License",
