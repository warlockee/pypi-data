    "0.2": {
        "info": {
            "author": "Franco Leonardo Bulgarelli",
            "author_email": "franco@mumuki.org",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Programming Language :: Python"
