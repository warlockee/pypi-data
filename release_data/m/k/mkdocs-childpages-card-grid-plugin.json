    "0.1.3": {
        "info": {
            "author": "Fausto Oliveira",
            "author_email": "",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
