    "0.4.0": {
        "info": {
            "author": "Rhet Turnbull",
            "author_email": "rturnbull@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
