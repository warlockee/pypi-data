        "info": {
            "author": "Audel Rouhi",
            "author_email": "knucklessg1@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Environment :: Console",
                "License :: Public Domain",
