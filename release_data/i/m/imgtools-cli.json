    "1.0.5": {
        "info": {
            "author": "3ee Games",
            "author_email": "ryguy@3ee.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
