    "0.0.3": {
        "info": {
            "author": "Timothy Law",
            "author_email": "timthlaw@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
