    "0.5.0": {
        "info": {
            "author": "Ludwig Lierhammer",
            "author_email": "ludwig.lierhammer@hereon.de",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 2 - Pre-Alpha",
                "Intended Audience :: Developers",
