    "2.0.1": {
        "info": {
            "author": "Filip Mroz",
            "author_email": "fafafft@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Science/Research",
