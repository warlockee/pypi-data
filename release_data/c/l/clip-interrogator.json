    "0.3.3": {
        "info": {
            "author": "pharmapsychotic",
            "author_email": "me@pharmapsychotic.com",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Developers",
                "Intended Audience :: Science/Research",
