    "0.1.2": {
        "info": {
            "author": "AkagiYui",
            "author_email": "akagiyui@yeah.net",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
