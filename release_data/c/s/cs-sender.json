    "1.1.2": {
        "info": {
            "author": "liuxianglong",
            "author_email": "liu_xianglong@live.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
