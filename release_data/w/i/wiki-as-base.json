    "0.4.1": {
        "info": {
            "author": "Emerson Rocha",
            "author_email": "rocha@ieee.org",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: The Unlicense (Unlicense)",
                "Operating System :: OS Independent",
                "Programming Language :: Python :: 3",
                "Topic :: Utilities",
                "Typing :: Typed"
