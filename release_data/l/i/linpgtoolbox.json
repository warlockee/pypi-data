    "22.11": {
        "info": {
            "author": "Linpg Foundation",
            "author_email": "yudong9912@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Operating System :: OS Independent",
                "Programming Language :: Python :: 3.10"
