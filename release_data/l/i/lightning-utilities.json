    "0.4.1": {
        "info": {
            "author": "Lightning AI et al.",
            "author_email": "pytorch@lightning.ai",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Console",
