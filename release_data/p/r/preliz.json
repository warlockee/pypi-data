    "0.1.1": {
        "info": {
            "author": "",
            "author_email": "ArviZ team <arviz.devs@gmail.com>",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Intended Audience :: Education",
