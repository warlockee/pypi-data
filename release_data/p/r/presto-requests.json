    "0.3.6": {
        "info": {
            "author": "Phillip Sitbon",
            "author_email": "phillip.sitbon@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Programming Language :: Python :: 3",
                "Programming Language :: Python :: 3.10",
