    "0.1.16": {
        "info": {
            "author": "Oleg Matskiv",
            "author_email": "alpaca00tuha@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Framework :: Pytest",
                "Intended Audience :: Developers",
                "License :: OSI Approved :: MIT License",
                "Natural Language :: English",
