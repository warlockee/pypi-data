    "0.1.1": {
        "info": {
            "author": "Usman Musa",
            "author_email": "usmanmusa1920@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
