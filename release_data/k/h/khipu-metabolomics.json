    "0.4.6": {
        "info": {
            "author": "Shuzhao Li",
            "author_email": "shuzhao.li@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
