    "1.2": {
        "info": {
            "author": "Lakera AI",
            "author_email": "dev@lakera.ai",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Operating System :: OS Independent",
