    "0.3.2": {
        "info": {
            "author": "Ought",
            "author_email": "team@ought.org",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3.10"
