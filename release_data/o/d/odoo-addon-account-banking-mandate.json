    "15.0.1.0.1.3": {
        "info": {
            "author": "Compassion CH, Tecnativa, Akretion, Odoo Community Association (OCA)",
            "author_email": "support@odoo-community.org",
            "bugtrack_url": null,
            "classifiers": [
