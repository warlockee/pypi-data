    "14.0.1.2.2.dev3": {
        "info": {
            "author": "ForgeFlow, Odoo Community Association (OCA)",
            "author_email": "support@odoo-community.org",
            "bugtrack_url": null,
            "classifiers": [
                "Framework :: Odoo",
                "Framework :: Odoo :: 14.0",
