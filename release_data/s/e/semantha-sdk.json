    "4.2.1": {
        "info": {
            "author": "Tom Kaminski",
            "author_email": "tom.kaminski@semantha.de",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: Apache Software License",
                "Programming Language :: Python :: 3",
