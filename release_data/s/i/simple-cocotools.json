    "0.1.1": {
        "info": {
            "author": "Frank Odom",
            "author_email": "fodom@plainsight.ai",
            "bugtrack_url": null,
            "classifiers": [
                "Operating System :: OS Independent",
                "Programming Language :: Python :: 3"
