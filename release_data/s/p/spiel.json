    "0.4.3": {
        "info": {
            "author": "JoshKarpel",
            "author_email": "josh.karpel@gmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Console",
                "Intended Audience :: Developers",
