    "0.0.4.1": {
        "info": {
            "author": "triwinds",
            "author_email": "triwinds@foxmail.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 4 - Beta",
                "Intended Audience :: Developers",
