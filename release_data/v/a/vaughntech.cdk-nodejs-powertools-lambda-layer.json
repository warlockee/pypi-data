        "info": {
            "author": "VaughnGit<alvin.vaughn@outlook.com>",
            "author_email": "",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 5 - Production/Stable",
                "Intended Audience :: Developers",
