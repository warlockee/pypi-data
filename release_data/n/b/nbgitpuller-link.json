    "0.2.2": {
        "info": {
            "author": "",
            "author_email": "Mark Piper <mark.piper@colorado.edu>",
            "bugtrack_url": null,
            "classifiers": [
                "Intended Audience :: Developers",
                "Intended Audience :: Education",
