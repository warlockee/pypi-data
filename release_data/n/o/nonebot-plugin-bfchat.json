    "0.2.0": {
        "info": {
            "author": "Nightsky",
            "author_email": "050644zf@outlook.com",
            "bugtrack_url": null,
            "classifiers": [
                "License :: OSI Approved :: MIT License",
                "Programming Language :: Python :: 3",
