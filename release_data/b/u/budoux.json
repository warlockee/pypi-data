    "0.2.1": {
        "info": {
            "author": "Shuhei Iitsuka",
            "author_email": "tushuhei@google.com",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "License :: OSI Approved :: Apache Software License",
