            "author": "Simples Consultoria",
            "author_email": "produtos@simplesconsultoria.com.br",
            "bugtrack_url": null,
            "classifiers": [
                "Development Status :: 3 - Alpha",
                "Environment :: Web Environment",
                "Framework :: Plone",
                "Framework :: Plone :: 4.3",
                "Framework :: Plone :: 5.0",
